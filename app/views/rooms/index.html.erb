<div class="header">
  <%= render "shared/header" %>
</div>
<div class="main">
  <%= "rooms#index" %>
  <%= link_to "新規グループ作成" ,new_room_path, class: "new_group" %>
  <%= form_with(url:  search_users_path, local: true,method: :get, class:"search-form") do |f| %>
    <%= f.text_field :user_name, placeholder: "ユーザーネームを入力してください",class: "search_input" %>
    <%= f.submit "検索", class: "search_btn" %>
  <% end %>  
</div>
