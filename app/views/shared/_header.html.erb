<div class="page-header">
  <div class="left-contents">
    <%= link_to "shovelu", root_path ,class: "shovel-icon" %>
    <%#= link_to image_tag("#", class:"shovelu-icon"), "/" %>
  </div> 
  <nav class="right-contents"> 
    <ul>     
      <% if user_signed_in? %>
        <li><%= link_to "#{current_user.nickname}さん", user_path(current_user.id), class: "user-nickname" %></li>
        <li><%= link_to "ログアウト", destroy_user_session_path,method: :delete, class: "logout" %></li>
      <% else %>
        <li><%= link_to 'ログイン', new_user_session_path, class: "nav-btn" %></li>
        <li><%= link_to "新規登録", new_user_registration_path, class: "nav-btn" %></li>
      <% end %>  
    </ul>   
  </div>   
</div>